{
  "name": "@civet/events",
  "version": "1.0.0-rc2",
  "description": "Civet",
  "main": "./lib/index.js",
  "scripts": {
    "build:clean": "rimraf ./lib",
    "prebuild": "npm run build:clean",
    "build": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "lint": "eslint src",
    "prepare": "npm run lint && npm run build",
    "preversion": "git diff HEAD --name-only --exit-code || (echo -\\> unclean working directory && exit 1)",
    "postversion": "cross-var git commit -am$npm_package_version",
    "prettify": "prettier --write src/**"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/civet-org/events.git"
  },
  "keywords": [
    "civet",
    "react",
    "data",
    "rest"
  ],
  "author": "Aaron Burmeister",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/civet-org/events/issues"
  },
  "homepage": "https://civet.js.org/",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@civet/core": "^1.0.0-rc4",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "eslint": "7.2",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "@civet/core": ">= 1.0.0",
    "react": ">= 16.8",
    "react-dom": ">= 16.8"
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3",
    "prop-types": "^15.8.1"
  }
